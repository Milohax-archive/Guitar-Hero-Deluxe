#define ERROR_SCREEN_HANDLERS
((panels dialog)
   (focus dialog)
   (allow_back FALSE)
   (animate_transition FALSE)
   (focus dl_button1.btn)
   (helpbar
      (default
         ((fret1 help_continue))))
   (enter
      {$this set_focus dl_button1.btn}
      {dl_button1.btn set_text CONTINUE}
      {dl_button2.btn set_showing FALSE}
      {dl_button2.btn set_state kDisabled}
      {dl_title.lbl
         set_text
         {$this title}}
      {dl_message.lbl
         set_text
         {$this message}})
   (SELECT_MSG
      {if
         {== $component dl_button1.btn}
         {ui pop_screen}}))
{new_gh_screen
   error_duplicate_profile
   ERROR_SCREEN_HANDLERS
   (title duplicate_profile)
   (message duplicate_profile_msg)}
{new_gh_screen
   error_no_profile
   ERROR_SCREEN_HANDLERS
   (title no_profile)
   (message no_profile_msg)}
{new_gh_screen
   error_max_profile
   ERROR_SCREEN_HANDLERS
   (title max_profile)
   (message max_profile_msg)}
{new_gh_screen
   error_cant_delete_profile
   ERROR_SCREEN_HANDLERS
   (title cant_delete_profile)
   (message cant_delete_profile_msg)}